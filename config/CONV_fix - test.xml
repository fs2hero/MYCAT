<?xml version="1.0" encoding="UTF-8"?>
<converter>
    <common>
        <targetFormat>txt</targetFormat>
        <badPath>C:/MYCAT/corpus/bad</badPath>
        <docPath>C:/MYCAT/corpus/docs</docPath>
        <sourcePath>C:/MYCAT/corpus/source</sourcePath>
        <tempPath>C:/MYCAT/temp</tempPath>
        <maxRetry>2</maxRetry>
        <keepExtension>true</keepExtension>
        <outputEncoding>UTF-8</outputEncoding>
    </common> 
	<!-- examples of plugins -->
  <plugins>
        <plugin>
            <plugName>antiword</plugName>
            <envVar name="ANTIWORDHOME">C:/MYCAT/Plugins/antiword</envVar>
            <plugCommand>C:/MYCAT/Plugins/antiword/antiword.exe %source% >> %target%</plugCommand>
            <plugProcessWindows>antiword.exe</plugProcessWindows>
        </plugin>
         <maps>
            <ext>
                <extName>doc</extName>
               <conv>antiword</conv>
                <builtin>no</builtin>
            </ext>
          </maps>
    </plugins>
	
</converter>
